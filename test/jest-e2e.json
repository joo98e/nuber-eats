{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "moduleNameMapper": {
    "^@modules/(.*)": [
      "<rootDir>/../src/$1"
    ],
    "^@common/(.*)": [
      "<rootDir>/../common/$1"
    ]
  },
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t)s$": "ts-jest"
  }
}
